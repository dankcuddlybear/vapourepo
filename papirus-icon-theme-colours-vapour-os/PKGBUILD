pkgname=papirus-icon-theme-colours-vapour-os
pkgver=$(date +%Y.%-m.%-d)
pkgrel=$(expr $(date +%-S) + $(expr 60 \* $(date +%-M)) + $(expr 3600 \* $(date +%-H)))
pkgdesc='Papirus modern icon theme with additional default folder colours'
arch=(x86_64)
url='https://github.com/dankcuddlybear/vapourepo'
depends=('papirus-icon-theme')
package() {
	cp -r "$srcdir/../files/usr" "$pkgdir"
}
