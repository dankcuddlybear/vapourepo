# Maintainer: Dankcuddlybear <dankcuddlybear@protonmail.com>
pkgname=lib32-vapour-os-multimedia-codecs
pkgver=$(date +%Y.%-m.%-d)
pkgrel=$(expr $(date +%-S) + $(expr 60 \* $(date +%-M)) + $(expr 3600 \* $(date +%-H)))
pkgdesc='Multimedia codecs all-in-one package for Vapour OS (32-bit)'
arch=(x86_64)
url='https://github.com/dankcuddlybear/Vapour-OS'
depends=(
	lib32-a52dec
	lib32-faac lib32-faad2 lib32-ffmpeg lib32-fluidsynth
	lib32-gst-plugins-base lib32-gst-plugins-good
	lib32-libavtp lib32-libcdio libdca lib32-libdc1394 lib32-libdvdcss lib32-libdvdread lib32-libdvdnav lib32-libfdk-aac lib32-libkate lib32-libmodplug lib32-libmpcdec lib32-libmpeg2 lib32-libtheora
	lib32-opencore-amr
	lib32-wildmidi
	lib32-zvbi
	vapour-os-multimedia-codecs
)
