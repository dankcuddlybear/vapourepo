pkgname=vapour-os-mirrorlist
pkgver=$(date +%Y.%-m.%-d_%-H.%-M.%-S)
pkgrel=1
pkgdesc='Vapourepo mirrorlist'
arch=(x86_64)
url='https://github.com/dankcuddlybear/vapourepo'
package() {
	cp -r "$srcdir/../etc" "$pkgdir"
	"$srcdir/../../set-default-permissions.sh" "$pkgdir"
}
